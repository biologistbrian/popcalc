<p id="notice"><%= notice %></p>

<h1>Projlines</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projlines.each do |projline| %>
      <tr>
        <td><%= link_to 'Show', projline %></td>
        <td><%= link_to 'Edit', edit_projline_path(projline) %></td>
        <td><%= link_to 'Destroy', projline, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Projline', new_projline_path %>
